{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"pocketbase:install:linux": " eval $(grep '^POCKETBASE_INSTALL_URL' .env) && curl -L -o backend/pb8 $POCKETBASE_INSTALL_URL && unzip backend/pb8 -d backend && rm backend/pb8",
		"pocketbase:start": "./backend/pocketbase serve"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^2.9.0",
		"@skeletonlabs/tw-plugin": "^0.3.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/eslint": "8.56.5",
		"@types/node": "^20.11.24",
		"@typescript-eslint/eslint-plugin": "^7.1.0",
		"@typescript-eslint/parser": "^7.1.0",
		"autoprefixer": "^10.4.18",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6",
		"sveltekit-superforms": "^2.7.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.4",
		"vitest": "^1.3.1"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.6.3",
		"@iconify/svelte": "^3.1.6",
		"pocketbase": "^0.21.1",
		"zod": "^3.22.4"
	}
}
